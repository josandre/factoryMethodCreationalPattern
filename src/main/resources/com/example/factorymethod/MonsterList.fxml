<AnchorPane prefHeight="400.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.factorymethod.View.MonsterList">
   <children>
      <TableView fx:id="tblMonsters" layoutX="122.0" layoutY="87.0" prefHeight="226.0" prefWidth="585.0">
        <columns>
          <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="colUpperExtremity" prefWidth="101.0" text="upperExtremity" />
            <TableColumn fx:id="colLowerExtremity" prefWidth="101.0" text="lowerExtremity" />
            <TableColumn fx:id="colBody" prefWidth="101.0" text="Body" />
            <TableColumn fx:id="colTail" prefWidth="101.0" text="Tail" />
            <TableColumn fx:id="colHabitat" prefWidth="101.0" text="Habitat" />
        </columns>
      </TableView>
      <Button fx:id="btnBack" layoutX="392.0" layoutY="345.0" mnemonicParsing="false" onAction="#handleBack" text="Back" textFill="#401111" />
      <Label layoutX="317.0" layoutY="33.0" text="Monster list" textFill="#6f1c1c">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <ImageView fitHeight="71.0" fitWidth="98.0" layoutX="496.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/eye.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="71.0" fitWidth="145.0" layoutX="239.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/zombie.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
